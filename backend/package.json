{
  "name": "@typeto.me/backend",
  "version": "0.2.0",
  "description": "real time chat client",
  "main": "app.js",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "format-check": "prettier --write \"src/**/*.ts\"",
    "type-check": "tsc --noEmit --disableSizeLimit",
    "lint-js": "eslint --ext ts ./src",
    "lint": "yarn format-check && yarn lint-js",
    "test": "exit 0",
    "clean": "rimraf dist",
    "dev": "nodemon -e ts --exec yarn ts-dev",
    "dev-brk": "nodemon -e ts --exec yarn ts-dev-brk",
    "ts-dev": "node -r esbuild-register --inspect=0.0.0.0:9229",
    "ts-dev-brk": "node -r esbuild-register --inspect-brk=0.0.0.0:9229",
    "build": "yarn node ./make_it_so.js",
    "build-tool": "yarn build && tsc --declaration --emitDeclarationOnly",
    "start-prod": "node ./dist/app.js",
    "start": "cross-env ENV_NAME=local yarn start-debug",
    "start-debug": "yarn dev ./src/app.ts",
    "run-dev": "node -r esbuild-register ./src/app.ts",
    "semantic-release": "semantic-release -e semantic-release-monorepo"
  },
  "dependencies": {
    "axios": "0.21.4",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "http-proxy-middleware": "2.0.1",
    "socket.io": "^4.2.0"
  },
  "devDependencies": {
    "@babel/cli": "7.15.7",
    "@babel/core": "7.15.5",
    "@babel/node": "7.15.4",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "7.15.6",
    "@babel/plugin-transform-runtime": "7.15.0",
    "@babel/preset-env": "7.15.6",
    "@babel/preset-typescript": "7.15.0",
    "@babel/register": "7.15.3",
    "@babel/runtime": "7.15.4",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/commit-analyzer": "^9.0.1",
    "@semantic-release/git": "^10.0.0",
    "@semantic-release/github": "^8.0.1",
    "@semantic-release/gitlab": "^7.0.3",
    "@semantic-release/npm": "^8.0.0",
    "@semantic-release/release-notes-generator": "^10.0.2",
    "@types/cookie-parser": "1.4.2",
    "@types/express": "4.17.13",
    "@types/jest": "^27.0.2",
    "@types/node": "14.17.19",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "@yarnpkg/esbuild-plugin-pnp": "1.0.1-rc.7",
    "cross-env": "^7.0.3",
    "esbuild": "0.13.2",
    "esbuild-register": "3.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-snakecasejs": "2.2.0",
    "glob": "7.2.0",
    "jest": "^27.2.2",
    "jest-environment-node": "^27.2.2",
    "nodemon": "2.0.13",
    "prettier": "^2.4.1",
    "rimraf": "3.0.2",
    "semantic-release": "^18.0.0",
    "semantic-release-monorepo": "^7.0.5",
    "typescript": "^4.4.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lysol/typeto.me"
  },
  "keywords": [
    "chat",
    "unix",
    "socket"
  ],
  "author": "Derek Arnold and Jordan Byrd",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lysol/typeto.me/issues"
  },
  "homepage": "https://github.com/lysol/typeto.me",
  "packageManager": "yarn@3.1.0-rc.6"
}
